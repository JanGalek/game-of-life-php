{
	"name": "galek/game-of-life",
	"description": "Game of Life",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">= 8.2",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.1",
		"nette/di": "^3.1",
		"nette/forms": "^3.1",
		"nette/http": "^3.2",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.1",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.9",
		"contributte/console": "^0.9.3"
	},
	"require-dev": {
		"symfony/thanks": "^1",
		"phpunit/phpunit": "^10",
		"contributte/qa": "^0.3.1",
		"contributte/phpstan": "^0.1.0",
		"mockery/mockery": "^1.6",
		"phpstan/phpstan": "^1.10"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"GameOfLife\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"bin-dir": "bin",
		"allow-plugins": {
			"symfony/thanks": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
